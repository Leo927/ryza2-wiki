rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2021, 2, 28);
    }
  }

  match /databases/item/documents {
    allow write: if id != null;
  }

  match /databases/map/documents {
    allow write: if id != null;
  }

}